{
    "name": "craftydigit/puff",
    "type": "library",
    "description": "PHP MVC Framework",
    "keywords": [
        "php",
        "framework",
        "puff"
    ],
    "homepage": "https://github.com/CraftyDigit/puff",
    "license": "MIT",
    "authors": [
        {
            "name": "Vyacheslav Oleinik",
            "email": "oleinik.v89@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "psr/container": "^2.0.2",
        "psr/event-dispatcher": "^1.0.0",
        "psr/http-server-handler": "^1.0.2",
        "doctrine/orm": "^2.11.0",
        "doctrine/dbal": "^3.2",
        "symfony/cache": "^6.2",
        "twig/twig": "^3.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "CraftyDigit\\Puff\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CraftyDigit\\Puff\\Tests\\": "Tests/"
        }
