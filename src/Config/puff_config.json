{
  "mode": "dev",
  "sessions": {
    "enabled": true
  },
  "di_container": {
    "services": {
      "Psr\\Http\\Message\\ResponseInterface": "GuzzleHttp\\Psr7\\Response",
      "Psr\\EventDispatcher\\EventDispatcherInterface": "CraftyDigit\\Puff\\EventDispatcher\\EventDispatcher",
      "CraftyDigit\\Puff\\Container\\ContainerExtendedInterface": "CraftyDigit\\Puff\\Container\\Container",
      "CraftyDigit\\Puff\\Controller\\ControllerManagerInterface": "CraftyDigit\\Puff\\Controller\\ControllerManager",
      "CraftyDigit\\Puff\\ErrorReporter\\ErrorReporterInterface": "CraftyDigit\\Puff\\ErrorReporter\\ErrorReporter",
      "CraftyDigit\\Puff\\Model\\ModelInterface": "CraftyDigit\\Puff\\Model\\Model",
      "CraftyDigit\\Puff\\Router\\RouteManagerInterface": "CraftyDigit\\Puff\\Router\\RouteManager",
      "CraftyDigit\\Puff\\Template\\TemplateInterface": "CraftyDigit\\Puff\\Template\\Template",
      "CraftyDigit\\Puff\\DataHandler\\DataHandlerInterface": "CraftyDigit\\Puff\\DataHandler\\DataHandler",
      "CraftyDigit\\Puff\\Template\\TemplateEngineManagerInterface": "CraftyDigit\\Puff\\Template\\TemplateEngineManager",
      "CraftyDigit\\Puff\\Http\\HttpManagerInterface": "CraftyDigit\\Puff\\Http\\HttpManager",
      "CraftyDigit\\Puff\\Session\\SessionInterface": "CraftyDigit\\Puff\\Session\\Session",
      "CraftyDigit\\Puff\\EventDispatcher\\EventDispatcherHelperInterface": "CraftyDigit\\Puff\\EventDispatcher\\EventDispatcherHelper",
      "CraftyDigit\\Puff\\EventDispatcher\\ListenerManagerInterface": "CraftyDigit\\Puff\\EventDispatcher\\ListenerManager",
      "CraftyDigit\\Puff\\Middleware\\MiddlewareManagerInterface": "CraftyDigit\\Puff\\Middleware\\MiddlewareManager"
    }
  },
  "additional_config_files": [
    "puff_config"
  ],
  "middlewares": {
    "disabled": []
  },
  "default_data_handler": "doctrine",
  "doctrine": {
    "cache_enabled": true,
    "cache_dir": "cache/doctrine",
    "cache_lifetime": 3600
  },
  "default_template_engine": "twig",
  "template_extension": "php",
  "twig": {
    "cache": "cache/twig",
    "template_extension": "html.twig",
    "debug": true
  },
  "http_client": {
    "default": "guzzle",
    "default_server_request_provider_method": "\\GuzzleHttp\\Psr7\\ServerRequest::fromGlobals"
  },
  "guzzle": {
    "base_uri": "",
    "timeout": 2.0
  },
  "xdebug": {
    "use_exception_message": true
  }
}