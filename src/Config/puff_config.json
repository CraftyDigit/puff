{
  "mode": "dev",
  "sessions": {
    "enabled": true
  },
  "di_container": {
    "services": {
      "CraftyDigit\\Puff\\Container\\ContainerExtendedInterface": "CraftyDigit\\Puff\\Container\\Container",
      "CraftyDigit\\Puff\\Controller\\ControllerManagerInterface": "CraftyDigit\\Puff\\Controller\\ControllerManager",
      "CraftyDigit\\Puff\\ErrorReporter\\ErrorReporterInterface": "CraftyDigit\\Puff\\ErrorReporter\\ErrorReporter",
      "CraftyDigit\\Puff\\Model\\ModelInterface": "CraftyDigit\\Puff\\Model\\Model",
      "CraftyDigit\\Puff\\Router\\RouterInterface": "CraftyDigit\\Puff\\Router\\Router",
      "CraftyDigit\\Puff\\Template\\TemplateInterface": "CraftyDigit\\Puff\\Template\\Template",
      "CraftyDigit\\Puff\\DataHandler\\DataHandlerInterface": "CraftyDigit\\Puff\\DataHandler\\DataHandler",
      "CraftyDigit\\Puff\\Template\\TemplateEngineManagerInterface": "CraftyDigit\\Puff\\Template\\TemplateEngineManager"
    }
  },
  "additional_config_files": [
    "puff_config"
  ],
  "default_data_handler": "doctrine",
  "doctrine": {
    "cache_enabled": true,
    "cache_dir": "cache/doctrine",
    "cache_lifetime": 3600
  },
  "default_template_engine": "twig",
  "template_extension": "php",
  "twig": {
    "cache": "cache/twig",
    "template_extension": "html.twig",
    "debug": true
  }
}